/*~~ Colors ~~*/
$darkBrown = #422A11 
$brown = #5B4830
$lightBrown = #D9C6A8
$green = #9FD6C2
$error = #D96F50

gradientGreen()
  background: #79c5a9
  background: rgb(146,208,184) /* Old browsers */
  background: linear-gradient(top, rgba(146,208,184,1) 0%,rgba(121,197,169,1) 100%)

gradientBrown()
  background: #9b693f
  background: rgb(156,129,106) /* Old browsers */
  background: linear-gradient(top, rgba(156,129,106,1) 0%,rgba(155,105,63,1) 100%)

gradientWhite()
  border: 1px solid white
  color: #3C2613
  border-radius: 5px
  box-shadow: 0 0px 5px rgba(0, 0, 0, 0.36), 0 0 1px 0 rgba(255, 255, 255, 1) inset
  background: white
  background: linear-gradient(top, rgba(255, 255, 255, 1), rgba(243, 240, 233, 1))
  // background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255, 255, 255, 1)), color-stop(100%,rgba(243, 240, 233, 1)))

btnGreen()
  border: 1px solid rgba(255,255,255,.3)
  color: #fff
  text-align: center
  text-shadow: 0 0 2px #000
  text-shadow: 0 0 2px rgba(0,0,0,0.7)
  box-shadow: 0 2px 3px rgba(0, 0, 0, 0.24), 0 1px 0 rgba(255, 255, 255, 0.24) inset
  gradientGreen()
  border-radius: 15px
  box-shadow: 0 0 5px rgba(0,0,0,.8)

btnWhiteEdit()
  line-height: 21px
  height: 20px
  background: white
  padding: 0 9px
  font-size: 12px
  margin-left: 10px
  position: relative
  left: 5px
  top: -2px
  &:hover
    border: 1px solid lighten(black, 25%)

// Generic Checkout Mixins
transparentWhite()
  background: #f0efeb
  background: url(/img/transparent-white.png) repeat;
  background: rgba(255,255,255,.6)
  box-shadow: 0 0 3px 0 rgba(0,0,0,.3)
  border-radius: 2px
transWhiteSection()
  clear: both
  padding: 10px 0
  margin-top: 15px
  transparentWhite()
  width: 746px
  margin-left: -18px

