/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
/** Inputs
/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
.text input, 
.password input,
textarea
  background: white
  background: rgba(255,255,255,.65)
  border: 3px solid $lightBrown
  box-sizing: border-box
  color: #422A11
  display: block
  font-family: 'News Cycle', arial, sans-serif
