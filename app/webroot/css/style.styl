/** Launch Page
/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
#launch
  h2,h3
    color: white
    font-family: 'Arimo', arial, sans-serif
    text-shadow: 1px 1px 2px rgba(0,0,0,.55)
    font-weight: 300
  h2
    font-size: 48px
    
    position: relative
    top: 10px
    margin: 0 auto
    strong
      color: $brown
      font-family: 'ChunkFiveRegular'
      text-transform: uppercase
      text-shadow: 0 0 5px white
      text-shadow: 0 0 5px rgba(255, 255, 255, .8)
    img
      padding: 0 8px
      position: relative
      top: -5px
  h3
    font-size: 30px
    margin: 10px 0 20px
  .container
    text-align: center
    margin: 0 auto
    width: 800px
    .ajax-content
      display: none
      margin: 200px auto 0
      width: 700px
      padding: 0
    #home-content
      display: block
      margin-bottom: 20px
      width: 800px
      h1 
        margin: 0
        .logo img
          height: 110px
          width: 495px
      form#UserLaunchSubmitForm
        position: relative
        display: inline-block
        // margin: 0 auto
        // position: relative
        // right: 15px
        // width: 365px
        .input.text
          position: relative
        #UserEmail
          background-image: none
          background-color: rgba(255,255,255, .55)
          font-size: 24px
          padding: 10px
          position: relative
          margin: 0 auto
          right: 0
          width: 355px
          transition: width .75s 0s ease-in-out
          &:focus,&.typed
            transition: width .75s 0s ease-in-out
            width: 425px
        button.submit-email
          background-color: #ffffff
          background-image: url(/img/button_cup.gif)
          background-position: center center
          background-repeat: no-repeat
          border-radius: 5px
          border: 1px solid #e5e5e5
          border: 1px solid rgba(255,255,255,.8)
          color: #fffff
          cursor: pointer
          display: inline
          font-family: Helvetica, arial, sans-serif
          font-size: 18px
          font-weight: bold
          height: 40px
          line-height: 35px
          position: absolute
          right: 10px
          text-align: center
          text-shadow: none
          top: 10px
          transition: width.75s 0s ease-in-out
          width: 55px
          &:hover,&.hover
            background-color: #F1EEF4
            box-shadow: 0 0 5px rgba(0,0,0,.3)
          &:active,&.active
            background-color: #E7E4EA
            border: 1px solid #422a11
            border: 1px solid rgba(0,0,0, .1)
            box-shadow: 0 0 5px rgba(0,0,0, .7) inset
          &.entered
              background-color: #ffffff
              position: absolute
              width: 95px
              transition: width .75s 0s ease-in-out
      section.thankyou
        display: none
        textarea
          display: block
          background: rgba(0,0,0,.6)
          border: 1px solid  rgba(255,255,255,.2)
          max-width: 350px
          min-width: 350px
          width: 350px
          margin: 20px auto 10px
          text-align: center
          resize: none
          border-radius: 4px
          font-size: 30px
          color: white
          text-shadow: 1px 1px 2px rgba(0,0,0,.55)
        #twitter-share-container #twitter-share
          display: inline-block
          font-family: 'Arimo'
          font-size: 20px
          font-weight: bold
          text-shadow: 0 0 2px #000
          text-shadow: 0 0 2px rgba(0,0,0,0.7)
          color: #000000
          background: url(/img/twitter-bird.png) no-repeat 10px center #ffffff
          border: 1px solid #fff
          box-shadow: 0 0 10px 0 rgba(0,0,0,.5)
          padding: 5px 5px 5px 35px
          width: 100px
          border-radius: 20px
          &:hover
            box-shadow: 0 0 5px 0 rgba(255,255,255,.8);
      span.error-message
        background: #fff
        color: #D96F50
        font-family: Arimo, arial
        font-size: 18px
        font-weight: bold
        padding: 15px 10px 10px
        margin: 5px auto
        box-shadow: 0 0 7px rgba(0,0,0,0.5)
        text-shadow: 1px 1px 0px #000
        text-shadow: 1px 1px 0px rgba(0,0,0,0.3)
        border-radius: 5px

/*~~ Laucnh Page - Shadow Inset Boxes ~~*/
.shadow-inset
  border: 1px solid rgba(255,255,255,.15)
  background: #333
  background: url(/img/50_black.png) repeat
  background: rgba(0,0,0,.1)
  font-family: Helvetica, Arial
