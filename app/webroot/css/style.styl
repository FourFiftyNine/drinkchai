  text-shadow: 0 0 3px rgba(0, 0, 0, .55)
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.610) inset
  border-radius: 5px
  &:focus
    border-color: $green


.required input,
.required textarea
  background-image: url(/img/asterisk.png)
  background-repeat: no-repeat
  background-position: 97%

.error input
  background-repeat: no-repeat
  background-position: 97%
  
/*~~ Inputs - Errors ~~*/
input.form-error,
textarea.form-error
  border: 3px solid $error
  background-image: url(/img/asterisk_error.png)

#authMessage
  background: $error
  font-size: 14px
  background: rgba(217, 111, 80, .8)
