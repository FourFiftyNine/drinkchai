/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
/** Backgrounds
/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
body
  /*background: url('/img/middle_bg.jpg') repeat*/
  color: #000000
  background: url(/img/tea_leaves.jpg) no-repeat center center fixed
  background-repeat: no-repeat
  background-position: center center
  background-attachment: fixed
  background-size: cover
  behavior: url(/js/libs/pie/PIE.htc)

.no-backgroundsize
  background: url(/img/tea_leaves.jpg) repeat-x center center fixed


/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
/** Inputs
/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
.text input,
textarea
  background: white
  border: 3px solid $lightBrown
  box-sizing: border-box
  color: #422A11
  display: block
  font-family: 'News Cycle', arial, sans-serif
  padding: 5px 10px
  text-shadow: 0 0 3px black
  text-shadow: 0 0 3px rgba(0, 0, 0, .55)
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.610) inset
  border-radius: 5px
  behavior: url(/js/libs/pie/PIE.htc)
  &:focus
    border-color: $green


.required input
  background-image: url(/img/asterisk.png)
  background-repeat: no-repeat
  background-position: 97%

/*~~ Inputs - Errors ~~*/
input.form-error
  border: 3px solid $error
  background-image: url(/img/asterisk_error.png);


