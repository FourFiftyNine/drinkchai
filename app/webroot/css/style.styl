/*~~ Colors ~~*/
$brown = #422A11
$lightBrown = #D9C6A8
$green = #9FD6C2
$error = #D96F50

gradientGreen()
  background: #79c5a9
  background: rgb(146,208,184) /* Old browsers */
  background: linear-gradient(top, rgba(146,208,184,1) 0%,rgba(121,197,169,1) 100%)
@import 'import/nib'
@import 'import/_h5bp'
// @import 'import/_custom_chosen'


/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
/** General Styles
/*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
html, button, input, select, textarea
  font-family: 'museo-slab', serif
body
  smooth()
  overflow-y: scroll

::-moz-selection
  background: #914c5a
  color: #fff
  text-shadow: none

::selection
  background: #914c5a
  color: #fff
  text-shadow: none

#spinner
  position: absolute !important
  left: 50% !important
  // top: 230px !important
  top: 50% !important

ul
  list-style-position: outside
  margin-top: 0
  padding-left: 20px
  list-style-image: url(/images/bullet.png)
  li
    font-size: 17px
.lt-ie9
  ul
    list-style-image: none
/*~~ Links ~~*/
a,
a:visited
  text-decoration: none
a:active
  position: relative
  top: 1px

a, a:hover, a:active, :focus
  outline: 0

transparentPng()
  background: transparent
  -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#00FFFFFF,endColorstr=#00FFFFFF)" /* IE8 */   
  zoom: 1

/*~~ General - Green Tag ~~*/

.green-tag
  display: block
  background: url(/img/tag.png) no-repeat
  cursor: pointer
  height: 169px
  width: 74px
  position: absolute
  right: -90px
  top: -5px

  &:hover
    top: 2px
  .facebook, .twitter
    cursor: pointer
    &:hover
      opacity: .5
    img
      opacity: 1
      position: relative
      left: -5
      left: -3px

  .facebook img
    top: 76px

#launch .green-tag
  right: 10px


