  font-weight: bold
  text-shadow: rgba(0,0,0,.4) 1px 1px 1px
  border-radius: 5px
  padding: 10px
  margin-top: 0
  margin-bottom:10px

.input
  position: relative // for error messages - Better way?
.error-message
  background: $error
  background: rgba(217, 111, 80, 0.8)
  border-radius: right 4px
  color: white
  cursor: pointer
  text-shadow: rgba(0, 0, 0, .4) 1px 1px 1px
  font-weight: bold
  font-size: 13px
  padding: 0 10px
  height: 35px
  line-height: 35px
  position: absolute
  right: 0
  top: 25px
  z-index: 100

.error-message.file
  position: relative
